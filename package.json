{
  "name": "raycast-rider-extension",
  "title": "JetBrains Rider Projects",
  "description": "Open recent JetBrains Rider projects",
  "icon": "rider-icon.png",
  "author": "anthea.wu14",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
